<table cellspacing="0" cellpadding="0" border="0" class="babsTable">
	<tr>
		<td class="babsHeader">
			<a href="$story_url">$headline</a></td>
	</tr>
	<tr>
		<td class="babsData1">
			<span class="babsInfo">
				Posted by <a href="$profile_url">$author</a>
				on $date</span></td>
	</tr>
	<tr>
		<td class="babsData2">
			$overview</td>
	</tr>
	<tr>
		<td class="babsFooter">
			<a href="$story_url">More Details</a>
			<span class="babsInfo">
			($comments comment(s),
			 $words words(s) in story)</span></td>
	</tr>
</table>
